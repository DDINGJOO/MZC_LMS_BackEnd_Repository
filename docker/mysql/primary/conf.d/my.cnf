[mysqld]
# Server ID for replication
server-id = 1

# Binary logging for replication
log-bin = mysql-bin
binlog-format = ROW
binlog-do-db = lms_db

# Character set
character-set-server = utf8mb4
collation-server = utf8mb4_unicode_ci

# Performance tuning
max_connections = 200
innodb_buffer_pool_size = 256M
innodb_log_file_size = 64M
